# RabbitMQ Configuration
RABBITMQ_USER=rabbitmq_user
RABBITMQ_PASSWORD=strong_rabbitmq_password

# Database Configurations
IDENTITY_DB_USER=identity_user
IDENTITY_DB_PASSWORD=strong_identity_db_password

ORDER_DB_USER=order_user
ORDER_DB_PASSWORD=strong_order_db_password

PAYMENT_DB_USER=payment_user
PAYMENT_DB_PASSWORD=strong_payment_db_password

# ASP.NET Core Environment
ASPNETCORE_ENVIRONMENT=Development

# Certificate Password
CERT_PASSWORD=strong_certificate_password

# Resource Limits (optional, can be adjusted as needed)
RABBITMQ_CPU_LIMIT=0.5
RABBITMQ_MEMORY_LIMIT=512M
RABBITMQ_CPU_RESERVATION=0.25
RABBITMQ_MEMORY_RESERVATION=256M

SERVICE_CPU_LIMIT=0.75
SERVICE_MEMORY_LIMIT=1G
SERVICE_CPU_RESERVATION=0.5
SERVICE_MEMORY_RESERVATION=512M

DB_CPU_LIMIT=0.5
DB_MEMORY_LIMIT=512M
DB_CPU_RESERVATION=0.25
DB_MEMORY_RESERVATION=256M

# Logging Configuration (optional)
LOG_MAX_SIZE=10m
LOG_MAX_FILE=3